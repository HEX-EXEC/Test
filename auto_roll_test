local a=_G.Spin;local b=game:GetService("Players").LocalPlayer;local c=game:GetService("VirtualInputManager");local d=game:GetService("GuiService");local e=b.PlayerGui.Interface.Customisation.Family.Buttons_2.Roll;local f=b.PlayerGui.Interface.Customisation.Family.Family.Title;local function g()return(f:IsA("TextLabel")or f:IsA("TextButton"))and f.Text or f.ContentText end;local function h()e.ZIndex=500;task.wait(.1);local i=e.AbsolutePosition;local j=e.AbsoluteSize;local k=d:GetGuiInset();local l=i.X+j.X/2+20;local m=i.Y+j.Y/2+k.Y;c:SendMouseButtonEvent(l,m,0,true,game,1);task.wait(.05);c:SendMouseButtonEvent(l,m,0,false,game,1)end;local n=g();h();f:GetPropertyChangedSignal((f:IsA("TextLabel")or f:IsA("TextButton"))and"Text"or"ContentText"):Connect(function()local o=g();if o~=n then n=o;if not string.find(o,a)then task.wait(.1);h()end end end)
